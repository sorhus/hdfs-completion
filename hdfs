_hdfs()
{
    DATA_LOC=/tmp/hdfs_completion

    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"

    if [[ ${prev} == -ls && -f $DATA_LOC ]]; then
        opts=$( cat $DATA_LOC )
        COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
        return 0
    fi
}
complete -F _hdfs hdfs
